# Use LLVM style for device related code
BasedOnStyle: LLVM
AlwaysBreakTemplateDeclarations: Yes

IncludeBlocks: Regroup
IncludeCategories:
  - Regex:           '^<.*\.h>'
    Priority:        1
  - Regex:           '^<.*'
    Priority:        2
  - Regex:           '.*\.pb\.h"$'
    Priority:        5
  - Regex:           '^"ppu.*'
    Priority:        4
  - Regex:           '^".*'
    Priority:        3
